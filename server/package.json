{
  "name": "ito-server",
  "version": "1.0.0",
  "main": "src/index.js",
  "type": "module",
  "license": "GPL-3.0-only",
  "dependencies": {
    "@grpc/grpc-js": "^1.13.4",
    "@grpc/proto-loader": "^0.7.15",
    "@grpc/reflection": "^1.0.4",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-oauth2-jwt-bearer": "^1.6.1",
    "express-rate-limit": "^7.5.0",
    "grpc-health-check": "^2.0.2",
    "pg": "^8.16.0"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "start": "node dist/index.js",
    "proto:gen": "proto-loader-gen-types --grpcLib=@grpc/grpc-js --outDir=src/generated/ src/ito.proto node_modules/grpc-health-check/proto/health/v1/health.proto && cp -r src/generated/ ../app/generated/",
    "dev": "tsx watch src/index.ts",
    "start:grpc": "tsx src/server.ts",
    "test-client": "tsx ./test-client.ts"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.0.1",
    "@types/pg": "^8.15.4",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.2",
    "typescript": "^5.8.3"
  }
}
